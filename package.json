{
    "name": "modern-js-portfolio",
    "version": "0.2.6",
    "description": "modern-js-portfolio",
    "main": "index.js",
    "scripts": {
        "test": "jest",        
        "start": "webpack serve --config webpack.dev.js",
        "build": "rimraf dist && rimraf build && webpack --config webpack.prod.js --mode production",
        "checkjs": "tsc --allowJs --checkJs --noEmit --moduleResolution node --target es6 src/*.js",
        "lint": "eslint src --ext .js && npm run checkjs",
        "serve": "serve",
        "clean:docs": "node ./scripts/rmrf.js ./docs",
        "jsdoc": "jsdoc -c jsdoc-config.json",
        "docmd": "jsdoc2md \"!(node_modules|__tests__)/**\" --template README_template.hbs --plugin @godaddy/dmd > README.md",
        "docs": "npm run clean:docs && npm run jsdoc && npm run docmd && npm run pretty",
        "pretty": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zloid/modern-js-portfolio.git"
    },
    "author": "idolz",
    "license": "MIT",
    "dependencies": {
        "normalizr": "^3.6.1",
        "redux": "^4.0.5",
        "reefjs": "^7.6.6"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@godaddy/dmd": "^1.0.3",
        "@testing-library/dom": "^7.29.4",
        "@testing-library/jest-dom": "^5.11.9",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "bootstrap": "^4.6.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.1",
        "docdash": "^1.2.0",
        "eslint": "^7.18.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.1",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "jest-cli": "^26.6.3",
        "jest-transform-stub": "^2.0.0",
        "jsdoc": "^3.6.6",
        "jsdoc-to-markdown": "^6.0.1",
        "mini-css-extract-plugin": "^1.3.5",
        "prettier": "^2.2.1",
        "purgecss": "^3.1.3",
        "purgecss-webpack-plugin": "^3.1.3",
        "redux-devtools-extension": "^2.13.8",
        "rimraf": "^3.0.2",
        "serve": "^11.3.2",
        "style-loader": "^2.0.0",
        "typescript": "^4.1.3",
        "webpack": "^5.18.0",
        "webpack-cli": "^4.4.0",
        "webpack-dev-server": "^3.11.2"
    },
    "engines": {
        "node": ">=14.15.1",
        "npm": ">=6.14.11"
    }
}
